#include <stdio.h>
int main(){
    printf("Calculate the volume of the cube \n");
    // variable for the side of the cube
    int a;
    // input from the user for the side of the cube
    printf("Enter the side of the cube",&a);
    scanf("%d",&a);
    // formula used for volume of the cube
    printf("Volume of the cube %d",a*a*a);
    return 0;
    // = )

}
    
