#include <stdio.h> 
int main() {
    // declaring a,b,c as an integer
    int a, b, c;
    
    
    printf("Enter a: ");
    scanf("%d", &a);  
    printf("Enter b: ");
    scanf("%d", &b);  

    // assigning a to c 
    c = a; 
    // assigning b to a the first variable a has been swapped
    a = b;
    /* assigning c to b (c has the value of a) both the variables have been swapped succesfully*/
    b = c;  
    printf("Value of a is %d\n", a);
    printf("Value of b is %d\n", b);
    printf("Both the variables have been swapped succesfully!")
    
    return 0;
}


