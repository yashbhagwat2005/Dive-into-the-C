#include <stdio.h>
int main(){
    printf("Convert Fahrenheit to Celsius\n");
    // Declaring farenheit and celsisus as a float
    float Fahrenheit,celsius;
    printf("Enter the tempreature in Fahrenheit: ");
    scanf("%f",&Fahrenheit);
    // formula used for converting farenheit to celsius
    celsius = (Fahrenheit-32)*5/9;
    printf("Celsius is %f",celsius);
    return 0;
    
}


// =)
