# include <stdio.h>
int main(){
    // declaring c a character variable 
    char c;
    // input from the user for a character
    printf("Enter a character  to check if it is a capital letter,lower case letter,special character or a digit:   ");
    scanf("%c",&c);
    // accesing the ascii value of the character
    int n;
    n = c;
    // to check if the character is capital letter,lower case letter,special character or a digit 
    if (n>=65 && n<=90)
        printf("%c is a capital letter",c);
    else if (n>=97 && n<=122)
        printf("%cis a lower case",c);
    else if (n>=128 && n<=255)
        printf("%c is a special symbol",c);
    else
        printf("%c is a digit",c);
        
        
    return 0;
}

// =)
