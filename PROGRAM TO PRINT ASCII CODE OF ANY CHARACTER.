# include <stdio.h>
int main(){
    // declaring c  as a character variable 
    char c;
    // input ffrom the user for a character
    printf("Enter a charecter  to know the ascii code of the character: ");
    scanf("%c",&c);
    // converting the character to an onteger to acces the ascii code
    int n;
    n = c;
    printf("\nThe ascii code for %c is %d",c,n);
    return 0;
}

